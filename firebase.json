{
  "hosting": {
    "public": "public",
    "headers": [{
      "source" : "/",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "no-cache"
      }]
    },{
      "source" : "/css/*.*",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "public, max-age=86400"
      }]      
    },{
      "source" : "/fonts/*.*",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "public, max-age=86400"
      }]      
    },{
      "source" : "/js/*.*",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "public, max-age=86400"
      }]      
    }]
  }
}
